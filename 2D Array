#include<stdio.h>
void main()
{
 int a[3][3]={{1,2,3}{4,5,6}{7,8,9}},i,j,trace=0;
 int **p;
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("\t a[%d][%d]=%d",i,j,*(*(a+i)+j));
  }
   printf("\n");
 }
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("\n Address a[%d][%d]=%u",i j,&a[i][j]);
  }
 }
   printf("\n");
   p='\0';
 for(i=0;i<3;i++)
 {
  p[i]=&a[i][0]
  for(j=0;j<3;j++)
  {
   printf("\n Address a[%d][%d]=%u",i,j,&p[i]+[j]);
  }
   printf("\n");
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("\t a[%d][%d]=%d",i,j,*(*(p+i)+j));
  }
   printf("\n");
 }
 for(i=0;i<3;i++)
 {
  trace+=a[i][i];
 }
   printf("Trace of the matrix is %d\n");
}
   
  
